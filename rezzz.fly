-- ======= UTILS =======
local function lerpColor(c1, c2, t)
	return Color3.new(
		c1.R + (c2.R - c1.R) * t,
		c1.G + (c2.G - c1.G) * t,
		c1.B + (c2.B - c1.B) * t
	)
end
local function clamp(v, a, b) if v < a then return a end if v > b then return b end return v end

