		local r = Instance.new("TextBox"); r.Size=UDim2.new(0,40,0,18); r.Position=UDim2.new(0,6,0,6); r.Text=tostring(math.floor(Settings.staticColor.R*255)); r.Parent=modal
		local g = Instance.new("TextBox"); g.Size=UDim2.new(0,40,0,18); g.Position=UDim2.new(0,52,0,6); g.Text=tostring(math.floor(Settings.staticColor.G*255)); g.Parent=modal
		local b = Instance.new("TextBox"); b.Size=UDim2.new(0,40,0,18); b.Position=UDim2.new(0,98,0,6); b.Text=tostring(math.floor(Settings.staticColor.B*255)); b.Parent=modal
		local ok = Instance.new("TextButton"); ok.Size=UDim2.new(0,50,0,18); ok.Position=UDim2.new(0,110,0,32); ok.Text="OK"; ok.Parent=modal
		ok.MouseButton1Click:Connect(function()
			local rn,gn,bn = tonumber(r.Text), tonumber(g.Text), tonumber(b.Text)
			if rn and gn and bn then
				Settings.staticColor = Color3.fromRGB(clamp(math.floor(rn),0,255), clamp(math.floor(gn),0,255), clamp(math.floor(bn),0,255))
				if Settings.enabled then applySelendang() end
			end
			modal:Destroy()
		end)
		-- auto remove on leaving
		task.delay(8, function() if modal and modal.Parent then modal:Destroy() end end)
	end)

	return screen
end

