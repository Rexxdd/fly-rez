-- ======= SETTINGS =======
local Settings = {
	enabled = false,
	mode = "Hands",          -- "Hands","Back","Head","Feet","Orbit","Ribbon"
	colorMode = "Rainbow",   -- "Rainbow","Static","Gradient"
	staticColor = Color3.fromRGB(255,255,255),
	gradA = Color3.fromRGB(255,100,0),
	gradB = Color3.fromRGB(255,0,255),
	length = 8.0,            -- panjang (meter-like)
	width = 1.0,             -- lebar (scale)
	animSpeed = 0.02,        -- warna anim speed
	trailLifetime = 0.7,
	maxCount = 120,
	rainbowTable = defaultColors,
	presetIndex = 1,
	orbitRadius = 2.5,       -- untuk Orbit mode
	orbitSpeed = 2.0
}

-- ======= STORAGE =======
local selendangParent = nil
local trails = {}           -- table of Trail instances
local attachments = {}      -- created attachments to cleanup
local animTask = nil
local orbitTask = nil

-- ======= CLEANUP =======
local function cleanup()
	if animTask then
		animTask:Cancel()
		animTask = nil
	end
	if orbitTask then
		orbitTask:Cancel()
		orbitTask = nil
	end
	for _, t in ipairs(trails) do
		if t and t.Parent then t:Destroy() end
	end
	trails = {}
	for _, a in ipairs(attachments) do
		if a and a.Parent then a:Destroy() end
	end
	attachments = {}
	if selendangParent and selendangParent.Parent then
		selendangParent:Destroy()
	end
	selendangParent = nil
end

-- ======= HELPERS TO FIND PARTS (R15 & R6 support) =======
local function findPart(char, names)
	for _, n in ipairs(names) do
		local p = char:FindFirstChild(n)
		if p then return p end
	end
	return nil
end

-- ======= CREATE TRAILS BASED ON MODE =======
local function createTrailsForMode(char)
	cleanup()
	if not char then return end

	-- parent invisible part to hold trails
	selendangParent = Instance.new("Part")
	selendangParent.Name = "SelendangParent"
	selendangParent.Size = Vector3.new(1,1,1)
	selendangParent.Transparency = 1
	selendangParent.Anchored = false
	selendangParent.CanCollide = false
	selendangParent.Parent = char

	local mode = Settings.mode

	-- local convenience
	local count = clamp(math.floor(Settings.length * 10), 4, Settings.maxCount)
	local spacing = -(Settings.length / math.max(count-1,1))

	-- Function to make a trail between two attachment parents with offsetZ
	local function makeTrail(parentA, parentB, idx, offsetZ, widthScale)
		-- create attachments on part A & B
		local attA = Instance.new("Attachment")
		attA.Name = ("Scarf_Att_%s_A_%d"):format(tostring(idx), idx)
		attA.Parent = parentA
		attA.Position = Vector3.new(0, 0, offsetZ)
		table.insert(attachments, attA)

		local attB = Instance.new("Attachment")
		attB.Name = ("Scarf_Att_%s_B_%d"):format(tostring(idx), idx)
		attB.Parent = parentB
		attB.Position = Vector3.new(0, 0, offsetZ)
		table.insert(attachments, attB)

		local tr = Instance.new("Trail")
		tr.Name = "ScarfTrail_"..idx
		tr.Attachment0 = attA
		tr.Attachment1 = attB
		tr.Lifetime = Settings.trailLifetime
		tr.LightEmission = 1
		tr.MinLength = 0.05
		tr.Transparency = NumberSequence.new(0,1)
		tr.WidthScale = NumberSequence.new(widthScale or Settings.width, 0)
		tr.Parent = selendangParent
		table.insert(trails, tr)
	end

	-- Different mode implementations
	if mode == "Hands" then
		local left = findPart(char, {"LeftHand","Left Arm"})
		local right = findPart(char, {"RightHand","Right Arm"})
		if not left or not right then return end
		for i=1,count do
			local offset = (i-1) * spacing
			makeTrail(left, right, i, offset, Settings.width)
		end

	elseif mode == "Back" then
		-- punggung: attach to UpperTorso/HumanoidRootPart as Attachment0 and a distant Attachment1 to create long trail
		local backPart = findPart(char, {"UpperTorso","Torso","HumanoidRootPart"})
		local anchor = Instance.new("Part")
		anchor.Name = "ScarfBackAnchor"
		anchor.Size = Vector3.new(1,1,1)
		anchor.Transparency = 1
		anchor.CanCollide = false
		anchor.Anchored = false
		anchor.Parent = selendangParent
		local weld = Instance.new("WeldConstraint")
		weld.Part0 = backPart
		weld.Part1 = anchor
		weld.Parent = anchor
		for i=1,count do
			local offset = (i-1) * spacing
			-- left-right attachments spaced slightly for width effect
			local att0 = Instance.new("Attachment", backPart); att0.Position = Vector3.new(-0.6, 0.5, 0)
			local att1 = Instance.new("Attachment", anchor); att1.Position = Vector3.new(-0.6, 0.5, offset)
			table.insert(attachments, att0); table.insert(attachments, att1)
			local tr = Instance.new("Trail")
			tr.Attachment0 = att0; tr.Attachment1 = att1
			tr.Parent = selendangParent
			tr.Lifetime = Settings.trailLifetime; tr.MinLength = 0.05; tr.LightEmission = 1
			tr.Transparency = NumberSequence.new(0,1)
			tr.WidthScale = NumberSequence.new(Settings.width,0)
			table.insert(trails, tr)
			-- second trail for right side
			local att2 = Instance.new("Attachment", backPart); att2.Position = Vector3.new(0.6, 0.5, 0)
			local att3 = Instance.new("Attachment", anchor); att3.Position = Vector3.new(0.6, 0.5, offset)
			table.insert(attachments, att2); table.insert(attachments, att3)
			local tr2 = Instance.new("Trail")
			tr2.Attachment0 = att2; tr2.Attachment1 = att3
			tr2.Parent = selendangParent
			tr2.Lifetime = Settings.trailLifetime; tr2.MinLength = 0.05; tr2.LightEmission = 1
			tr2.Transparency = NumberSequence.new(0,1)
			tr2.WidthScale = NumberSequence.new(Settings.width*0.8,0)
			table.insert(trails, tr2)
		end

	elseif mode == "Head" then
		local head = findPart(char, {"Head"})
		local root = findPart(char, {"HumanoidRootPart","Torso"})
		if not head or not root then return end
		for i=1,count do
			local offset = (i-1) * spacing
			makeTrail(head, root, i, offset, Settings.width*0.7)
		end

	elseif mode == "Feet" then
		local leftFoot = findPart(char, {"LeftFoot","LeftLeg","Left Leg"})
		local rightFoot = findPart(char, {"RightFoot","RightLeg","Right Leg"})
		if not leftFoot or not rightFoot then return end
		for i=1,count do
			local offset = (i-1) * spacing
			makeTrail(leftFoot, rightFoot, i, offset, Settings.width*0.6)
		end

	elseif mode == "Orbit" then
		-- create N attachments orbiting HumanoidRootPart and create trails between successive attachments to make ring-like trail
		local root = findPart(char, {"HumanoidRootPart","Torso","UpperTorso"})
		if not root then return end
		local orbitCount = clamp(math.floor(Settings.length * 4), 6, 60)
		local radius = Settings.orbitRadius
		for i=1,orbitCount do
			local ang = (i / orbitCount) * math.pi * 2
			local att = Instance.new("Attachment", root)
			att.Position = Vector3.new(math.cos(ang)*radius, 0.5, math.sin(ang)*radius)
			table.insert(attachments, att)
		end
		-- connect each pair to make continuous trail loop (using the same attachment as both ends to get ribbon effect)
		for i=1,#attachments do
			local a0 = attachments[i]
			local a1 = attachments[(i % #attachments) + 1]
			local tr = Instance.new("Trail")
			tr.Attachment0 = a0
			tr.Attachment1 = a1
			tr.Lifetime = Settings.trailLifetime
			tr.LightEmission = 1
			tr.MinLength = 0.02
			tr.Transparency = NumberSequence.new(0,1)
			tr.WidthScale = NumberSequence.new(Settings.width,0)
			tr.Parent = selendangParent
			table.insert(trails, tr)
		end
		-- orbit animation task (rotate attachments around)
		orbitTask = task.spawn(function()
			local t = 0
			while Settings.enabled and selendangParent and selendangParent.Parent do
				t = t + (Settings.orbitSpeed * 0.02)
				for i, att in ipairs(attachments) do
					local ang = (i / #attachments) * math.pi * 2 + t
					att.Position = Vector3.new(math.cos(ang)*radius, 0.5, math.sin(ang)*radius)
				end
				task.wait(0.03)
			end
		end)
		return
	elseif mode == "Ribbon" then
		-- wide ribbon: create multiple parallel trails between hands to create ribbon-like appearance
		local left = findPart(char, {"LeftHand","Left Arm"})
		local right = findPart(char, {"RightHand","Right Arm"})
		if not left or not right then return end
		local rows = clamp(math.floor(Settings.width * 3), 2, 10)
		for row = 1, rows do
			local yOffset = (row - (rows/2)) * 0.15
			for i=1,count do
				local offset = (i-1) * spacing
				-- create attachments with slight Y offsets
				local attL = Instance.new("Attachment", left); attL.Position = Vector3.new(0, yOffset, offset)
				local attR = Instance.new("Attachment", right); attR.Position = Vector3.new(0, yOffset, offset)
				table.insert(attachments, attL); table.insert(attachments, attR)
				local tr = Instance.new("Trail")
				tr.Attachment0 = attL; tr.Attachment1 = attR
				tr.Lifetime = Settings.trailLifetime; tr.MinLength = 0.02; tr.LightEmission = 1
				tr.Transparency = NumberSequence.new(0,1)
				tr.WidthScale = NumberSequence.new(Settings.width*0.9,0)
				tr.Parent = selendangParent
				table.insert(trails, tr)
			end
		end
		return
	end
end

-- ======= COLOR UPDATE / ANIMATION LOOP =======
local function startColorAnim()
	if animTask then
		animTask:Cancel()
		animTask = nil
	end
	if #trails == 0 then return end

	animTask = task.spawn(function()
		local step = 0
		while Settings.enabled and selendangParent and selendangParent.Parent do
			-- compute current color based on colorMode
			local curColor
			if Settings.colorMode == "Static" then
				curColor = Settings.staticColor
			elseif Settings.colorMode == "Gradient" then
				-- oscillate between gradA and gradB along step
				local tt = (math.sin(step * 2 * math.pi) + 1) / 2
				curColor = lerpColor(Settings.gradA, Settings.gradB, tt)
			else -- Rainbow
				local idx1 = math.floor(step) % #Settings.rainbowTable + 1
				local idx2 = (idx1 % #Settings.rainbowTable) + 1
				local t = step % 1
				curColor = lerpColor(Settings.rainbowTable[idx1], Settings.rainbowTable[idx2], t)
			end

			-- apply to all trails
			for _, tr in ipairs(trails) do
				tr.Color = ColorSequence.new(curColor)
				-- update width & lifetime in real-time
				tr.WidthScale = NumberSequence.new(Settings.width, 0)
				tr.Lifetime = Settings.trailLifetime
			end

			step = step + Settings.animSpeed
			task.wait(0.03)
		end
	end)
end

-- ======= APPLY / TOGGLE =======
local function applySelendang()
	local char = Player.Character
	if not char then return end
	if Settings.enabled then
		createTrailsForMode(char)
		startColorAnim()
	else
		cleanup()
	end
end

